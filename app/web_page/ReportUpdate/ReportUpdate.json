    },
    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.thesis && $data.comments && $data.comments_thesisID ){",
        "\t//attach lazy references variables to parent variables",
        "\t$data.comments.thesisID = $data.comments_thesisID;",
        "\treturn {",
        "\t\t//map thesis variable to expected task contract input",
        "\t\tthesisInput: {",
        "\t\t\treport: $data.thesis.report !== undefined ? $data.thesis.report : null",
        "\t\t},",
        "\t\t//map comments variable to expected task contract input",
        "\t\tcommentsInput: {",
        "\t\t\tresponse: $data.comments.response !== undefined ? $data.comments.response : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "thesis" : {
      "type" : "url",
      "value" : [
        "../{{context.thesis_ref.link}}"
      ],
      "exposed" : false
    },
    "comments_thesisID" : {
      "type" : "url",
      "value" : [
        "{{comments|lazyRef:'thesisID'}}"
      ],
      "exposed" : false
    },
    "context" : {
      "type" : "url",
      "value" : [
        "../API/bpm/userTask/{{taskId}}/context"
