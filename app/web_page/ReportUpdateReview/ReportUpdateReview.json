    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.thesis && $data.comments && $data.comments_thesisID ){",
        "\t//attach lazy references variables to parent variables",
        "\t$data.comments.thesisID = $data.comments_thesisID;",
        "\treturn {",
        "\t\t//map thesis variable to expected task contract input",
        "\t\tthesisInput: {",
        "\t\t\treportOK: $data.thesis.reportOK !== undefined ? $data.thesis.reportOK : null",
        "\t\t},",
        "\t\t//map comments variable to expected task contract input",
        "\t\tcommentsInput: {",
        "\t\t\tcomment: $data.comments.comment !== undefined ? $data.comments.comment : null,",
        "\t\t\tvalidated: $data.comments.validated !== undefined ? $data.comments.validated : null",
