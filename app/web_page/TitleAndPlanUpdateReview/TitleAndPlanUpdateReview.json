    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.thesis ){",
        "\treturn {",
        "\t\t//map thesis variable to expected task contract input",
        "\t\tthesisInput: {",
        "\t\t\ttitAndPlanOK: $data.thesis.titAndPlanOK !== undefined ? $data.thesis.titAndPlanOK : null,",
        "\t\t\tcomment: $data.thesis.comment !== undefined ? $data.thesis.comment : null,",
        "\t\t\tvalidated: $data.thesis.validated !== undefined ? $data.thesis.validated : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "thesis" : {
