    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.comments && $data.comments_thesisID && $data.thesis ){",
        "\t//attach lazy references variables to parent variables",
        "\t$data.comments.thesisID = $data.comments_thesisID;",
        "\treturn {",
        "\t\t//map comments variable to expected task contract input",
        "\t\tcommentsInput: {",
        "\t\t\tthesisID: $data.comments.thesisID ? {",
        "\t\t\t\tpersistenceId_string: $data.comments.thesisID.persistenceId_string !== undefined ? $data.comments.thesisID.persistenceId_string : null",
        "\t\t\t} : null,",
        "\t\t\tcomment: $data.comments.comment !== undefined ? $data.comments.comment : null",
        "\t\t},",
        "\t\t//map thesis variable to expected task contract input",
        "\t\tthesisInput: {",
        "\t\t\ttitAndPlanOK: $data.thesis.titAndPlanOK !== undefined ? $data.thesis.titAndPlanOK : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
